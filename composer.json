{
    "name": "novam-modules/web-builder",
    "type": "project",
    "description": "Web Builder Framework.",
    "keywords": [
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "server/app/",
            "Modules\\": "modules/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
